generator client {
      provider = "prisma-client-js"
    }

    datasource db {
      provider  = "postgresql"
      url       = env("DATABASE_URL") 
    }

    model User {
  id      Int  @id @default(autoincrement())
  nome    String
  idade   Int
  salario Float
} 

    model Category {
  id   Int    @id @default(autoincrement())
  name String
}